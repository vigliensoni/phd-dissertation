@inproceedings{schedl15influence,
	Address = {Vienna, Austria},
	Annote = {Main general findings from these results are that i) fusing scores of different recommenders frequently outperforms single variants, (ii) using the mean as aggregation function for CF almost always outperforms the maximum,2 and (iii) recommendations are overall better when categorizing users according to age and country than according to gender or genre.

