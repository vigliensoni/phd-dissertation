@book{bourdieu84distinction,
	Address = {Cambridge, MA},
	Annote = {In "Disctintion{\ldots}" Bourdieu discusses how those in power define aesthetic concepts such as taste. Referring to surveys of French citizens from different economic and educational backgrounds, he shows how social class tends to determine a person's likes and interests, and how distinctions based on social class get reinforced in daily life. He observes that even when the subordinate classes may seem to have their own particular idea of good taste, "the working-class 'aesthetic' is a dominated 'aesthetic' which is constantly obliged to define itself in terms of the dominant aesthetics..."

Bourdieu claims that aesthetic choices both create class-based social groups or "class fractions," and actively distance people in one class from another. Predispositions to certain kinds of food, music, and art are instilled at an early age in children and guide them to their appropriate social positions, by forcing them to internalize a preference for objects and behaviors that are suitable for them and an aversion towards the preferred objects and behaviors of other classes. When an individual encounters the culture or art of another class, he or she feels "disgust provoked by horror or visceral intolerance (`feeling sick') of the tastes of others."[3]

Taste is an important example of how class fractions are determined not only by social and economic capital, but also cultural capital, a more insidious mechanism for ensuring social and cultural reproduction. Because tastes are learned early and internalized to such a high degree, they are much more difficult to change and thus tend to permanently mark people of certain classes and impede their social mobility. In this way, the tastes of the dominant class tend to dominate those of the other classes, forcing individuals in economically and culturally dominated class strata to conform to certain aesthetic preferences at the risk of appearing crude, vulgar, or tasteless.},
	Author = {Bourdieu, Pierre},
	Date-Added = {2012-07-31 16:08:40 +0000},
	Date-Modified = {2016-12-07 01:59:17 +0000},
	Keywords = {Sociology, Culture},
	Publisher = {Harvard University Press},
	Rating = {5},
	Title = {Distinction: A Social Critique of the Judgement of Taste},
	Year = {1984},
	Bdsk-File-1 = {}}

@article{chamorro-premuzic07personality,
	Abstract = {This paper reports the results of a study on the relationship between individual differences and specific uses of music, referring to why and how people use music in everyday life. Questionnaire data from 341 respondents showed that open and intellectually engaged individuals, and those with higher IQ scores, tended to use music in a rational/cognitive way, while neurotic, introverted and non-conscientious individuals were all more likely to use music for emotional regulation (e.g. change or enhance moods). 

Results suggest that individual differences in personality and cognitive ability may partly determine the way in which we experience music. Limitations and suggestions for future studies are discussed.},
	Annote = {What?
This research is based on the findings by Rentfrow and Gosling, 2003, where they shed light about the use of music on everyday life, but it tackles an open question about the relationship between individual differences and different uses of music. The authors defined three major uses of music from the responses from the participants: emotional, cognitive, and background information. Finally, they correlate these uses with some of the traits of the participants. 

How?
The participants of the study did several tests on intelectual engagement, personality traits, and uses of music. Using PCA analysis, the authors defined three dimensions (emotional, cognitive, and background use of music) for the uses of music. Finally, they correlate the individuals traits with the use of music. 
Their results suggest that different uses of music are related to individual traits. Specifically, individuals with higher intelectual engagement are closer to listen music in a more cognitive way, while individuals with neuroticism listen music in a more emotional way.

To follow?
To define and test other traits that could define and modulate the individual music preferences},
	Author = {Chamorro-Premuzic, Tomas and Furnham, Adrian},
	Date-Added = {2012-07-31 14:43:43 +0000},
	Date-Modified = {2016-11-16 14:49:53 +0000},
	Journal = {British Journal of Psychology},
	Keywords = {Comps, Psychology, Social behaviour, Sociology},
	Pages = {175--85},
	Title = {Personality and music: Can traits explain how people use music in everyday life?},
	Volume = {98},
	Year = {2007},
	Bdsk-File-1 = {}}

@article{rentfrow03doremi,
	Abstract = {The present research examined individual differences in music preferences. A series of 6 studies investigated lay beliefs about music, the structure underlying music preferences, and the links between music preferences and personality. The data indicated that people consider music an important aspect of their lives and listening to music an activity they engaged in frequently. Using multiple samples, methods, and geographic regions, analyses of the music preferences of over 3,500 individuals converged to reveal 4 music-preference dimensions: Reflective and Complex, Intense and Rebellious, Upbeat and Conventional, and Energetic and Rhythmic. Preferences for these music dimensions were related to a wide array of personality dimensions (e.g., Openness), self-views (e.g., political orientation), and cognitive abilities (e.g., verbal IQ).},
	Annote = {What?
This article shows a series of six studies on beliefs about music, individual music preferences, and links between musical preferences and personality.
4 music-preference dimensions were found (reduced using PCA, and interpreted): Reflective and complex, Intense and rebellious, Upbeat and conventional, Energetic and rhythmic. Preferences for these dimensions were related to personality dimensions, self-views, and cognitive abilities

How?
A series of studies on music preferences were conducted, and more than 3,500 individuals were tested. Direct questions, but also investigating the online music libraries of people, were used: 
      1. To determine the importance of music in people's everyday life
      2. To determine the dimensions of music-preference and its attributes
      3. To determine how music-preference relates to existing personality characteristics


To follow:
The way the studies and experiments were designed and analyzed. Also, the music categorization they developed based on music genres, and the structure of music preferences they organized: reflective/complex, intense/rebelious, upbeat/conventional, and energetic/rhythmic. 
Also, the authors did not explore how individuals differences affect the way people use music. This is tackled by Chamorro-Premuzic and Furnham, 2007.},
	Author = {Rentfrow, Peter J. and Gosling, Samuel D.},
	Date-Added = {2012-07-30 20:56:20 +0000},
	Date-Modified = {2016-09-12 15:44:37 +0000},
	Journal = {Journal of Personality and Social Psychology},
	Keywords = {Music Preference, Comps, Factor Analysis},
	Number = {6},
	Pages = {1236--56},
	Title = {The do re mi's of everyday life: The structure and personality correlates of music preferences},
	Volume = {84},
	Year = {2003},
	Bdsk-File-1 = {}}

@article{nieminen11development,
	Abstract = {In the field of psychology, the first studies in experimental aesthetics were conducted approximately 140 years ago. Since then, research has mainly concentrated on aesthetic responses to visual art. Both the aesthetic experience of music and, especially, its devel- opment have received rather limited attention. Moreover, until now, very little attention has been paid to the investigation of the aesthetic experience of music using neurosci- entific methods. Aesthetic experiences are multidimensional and include inter alia sensory, perceptual, affective, and cognitive components. Aesthetic processes are usually experienced as pleasing and rewarding and are, thus, important and valuable experiences for many people. Because of their multidimensional nature, these processes employ several brain areas. In the present review, we examine important psychological and neural mechanisms that are believed to contribute to the development of aesthetic experiences of music. We also discuss relevant research findings. With the present review, we wish to provoke further discussion and possible future investigations as we consider the investi- gation of aesthetic experiences to be important both scientifically and with respect to potential clinical applications.},
	Author = {Nieminen, S., and Istok, E., and Brattico, E., and Tervaniemi, M., and Huotilainen, M.},
	Date-Added = {2011-11-27 00:06:41 +0000},
	Date-Modified = {2016-11-11 19:04:06 +0000},
	Journal = {Cortex},
	Keywords = {Aesthetic emotion, Aesthetic Judgement, Music},
	Number = {2011},
	Pages = {1138--46},
	Title = {The development of aesthetic responses to music and their underlying neural and psychological mechanisms},
